{
  "openapi": "3.0.1",
  "info": {
    "title": "API V1",
    "version": "v1"
  },
  "paths": {
    "/v1/games": {
      "post": {
        "summary": "Get result of game",
        "tags": [
          "Game"
        ],
        "parameters": [
          {
            "name": "player_option",
            "in": "query",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Game",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "data": {
                      "type": "object",
                      "properties": {
                        "player_option": {
                          "type": "string"
                        },
                        "computer_option": {
                          "type": "string"
                        },
                        "result": {
                          "type": "string"
                        }
                      },
                      "required": [
                        "player_option",
                        "computer_option",
                        "result"
                      ]
                    }
                  },
                  "required": [
                    "data"
                  ]
                }
              }
            }
          },
          "422": {
            "description": "Unprocessable"
          }
        }
      }
    }
  },
  "servers": [
    {
      "url": "https://{defaultHost}",
      "variables": {
        "defaultHost": {
          "default": "www.example.com"
        }
      }
    }
  ]
}